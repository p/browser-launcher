#!/usr/bin/env ruby

# frozen_string_literal: true

# https://www.ghacks.net/2016/08/14/override-firefox-add-on-signing-requirement/
# https://firefox-source-docs.mozilla.org/mozbase/mozprofile.html
#
# X11 message boxes: yad, zenity
#
# Requirements:
# ruby-activesupport

begin
  require 'browser_launcher/fox'
rescue LoadError
  $: << File.join(File.dirname(__FILE__), '../lib')
  require 'browser_launcher/fox'
end

BrowserLauncher::Fox::Launcher.new.run
